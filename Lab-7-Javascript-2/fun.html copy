        /**
 * Prompts the user to enter grades between 0 and 20 and stops when a value outside this range is entered.
 * Counts the number of grades entered and calculates the average grade.
 */
function calculateAverageGrade() {
    let grades = [];
    let grade = prompt("Enter a grade between 0 and 20 (enter a value outside this range to stop):");
 
    while (grade >= 0 && grade <= 20) {
        grades.push(grade);
        grade = prompt("Enter a grade between 0 and 20 (enter a value outside this range to stop):");
    }
 
    let totalGrades = grades.length;
    let sumGrades = grades.reduce((acc, curr) => acc + parseInt(curr), 0);
    let averageGrade = sumGrades / totalGrades;
 
    console.log(`Number of grades entered: ${totalGrades}`);
    console.log(`Average grade: ${averageGrade}`);
}
 
// Call the function to start the grade calculation process
calculateAverageGrade();
