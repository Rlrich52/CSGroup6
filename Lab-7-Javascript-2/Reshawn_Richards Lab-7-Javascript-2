      <button id="changeTextBtn">Click me</button>
      <p id="displayText">Original Text</p>
  
      <script>
          // Get elements by ID
          const button = document.getElementById("changeTextBtn");
          const paragraph = document.getElementById("displayText");
  
          // Add event listener to the button
          button.addEventListener("click", function() {
              // Call the displayText function to change the paragraph's text
              changeText();
          });
  
          // Function to change the paragraph's text
          function changeText() {
              // Change the text to "Hello, JavaScript World!"
              paragraph.textContent = "Hello, JavaScript World!";
          }
      </script>
